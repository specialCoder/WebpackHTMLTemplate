{
  "presets": [
    ["@babel/preset-env", {
      // Disable polyfill transforms
      "useBuiltIns": "usage",
      "corejs": 2,
      // Do not transform modules to CJS
      "modules":false
    }],
    ["@babel/react"]
],
  "plugins":[
       ["@babel/plugin-transform-runtime", {
           "corejs": 2
       }],
       ["@babel/plugin-proposal-decorators", { 
         "legacy": true 
         }]
    ]
}